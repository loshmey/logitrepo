<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:sch="http://localhost:8080/invoice/schema"
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://localhost:8080/invoice/schema"
                  targetNamespace="http://localhost:8080/invoice/schema">
    <wsdl:types xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ih="http://localhost:8080/invoice/schema"
                   elementFormDefault="qualified" targetNamespace="http://localhost:8080/invoice/schema">
            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Invoice schema</xs:documentation>
            </xs:annotation>
            <!--  Invoice object  -->
            <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="Invoice">
                <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Invoice object.</xs:documentation>
                </xs:annotation>
                <xs:complexType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                        <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="requestDateTime"
                                    type="xs:string"/>
                        <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" maxOccurs="1" name="invoiceHeaderData"
                                    type="ih:InvoiceHeader"/>
                        <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" maxOccurs="unbounded"
                                    name="invoiceRecordData" type="ih:InvoiceRecord"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="SendInvoiceRequest">
                <xs:complexType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                        <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" ref="ih:Invoice"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="SendInvoiceResponse">
                <xs:complexType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                        <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="status" type="ih:Status"/>
                        <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="description" type="xs:string"/>
                        <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="timestamp" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <!--  Request Item: INVOICE HEADER  -->
            <xs:complexType xmlns:xs="http://www.w3.org/2001/XMLSchema" name="InvoiceHeader">
                <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                        Invoice header containing information about supplier, buyer and transaction.
                    </xs:documentation>
                </xs:annotation>
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="supplier" type="ih:Supplier"/>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="buyer" type="ih:Buyer"/>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="transaction" type="ih:Transaction"/>
                </xs:sequence>
            </xs:complexType>
            <!--  Request Item: INVOICE RECORD  -->
            <xs:complexType xmlns:xs="http://www.w3.org/2001/XMLSchema" name="InvoiceRecord">
                <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                        Invoice record containing information about merch name, amount, quantity, prices...
                    </xs:documentation>
                </xs:annotation>
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="recordNumber">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Record number, 3
                                    characters, number only
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,3}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="merchOrServiceName">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                    Merch or service name, 120 characters, letters only
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="preserve"/>
                                <xs:minLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:maxLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="120"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="[a-zA-Z]{0,120}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="amount">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Amount, decimal number in
                                    format 10,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,10}.{0,1}\d{2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="unitOfIssue">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Unit of issue, 6
                                    characters, letters only
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:minLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:maxLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="6"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="[a-zA-Z]{0,6}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="unitPrice">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Unit price, decimal number
                                    in format 10,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,10}.{0,1}\d{2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="value">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Value, decimal number in
                                    format 12,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,12}.{0,1}\d{2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="rebatePercentage">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Rebate percentage, decimal
                                    number in format 5,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,5}.{0,1}\d{2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="rebateAmount">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Rebate amount, decimal
                                    number in format 12,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,12}.{0,1}\d{2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="rebateDeducted">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Rebate deducted, decimal
                                    number in format 12,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,12}.{0,1}\d{2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="taxTotal">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Tax total, decimal number
                                    in format 12,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,12}.{0,1}\d{2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <!--  Request Item: INVOICE HEADER - SUPPLIER  -->
            <xs:complexType xmlns:xs="http://www.w3.org/2001/XMLSchema" name="Supplier">
                <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Supplier properties.
                    </xs:documentation>
                </xs:annotation>
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="supplierName">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Supplier name, max 255
                                    characters, letters only
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="preserve"/>
                                <xs:minLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:maxLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="255"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="[a-zA-Z ]{0,255}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="supplierAddress">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Supplier address, max 255
                                    characters, letters only
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="preserve"/>
                                <xs:minLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:maxLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="255"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="[a-zA-Z ]{0,255}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="supplierPIB">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                    Supplier PIB, 11 characters, numbers only (first character must not be zero
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:length xmlns:xs="http://www.w3.org/2001/XMLSchema" value="11"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="[1-9][0-9]{10}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <!--  Request Item: INVOICE HEADER - BUYER  -->
            <xs:complexType xmlns:xs="http://www.w3.org/2001/XMLSchema" name="Buyer">
                <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Buyer properties.</xs:documentation>
                </xs:annotation>
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="buyerName">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Buyer name, max 55
                                    characters, letters only
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:minLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:maxLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="255"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="[a-zA-Z ]{0,55}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="buyerAddress">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Buyer address, max 55
                                    characters, alphanumeric
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:minLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:maxLength xmlns:xs="http://www.w3.org/2001/XMLSchema" value="255"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="[a-zA-Z0-9 ]{0,55}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="buyerPIB">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                    Buyer PIB, 11 characters, numbers only (first character must not be zero
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:string">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:length xmlns:xs="http://www.w3.org/2001/XMLSchema" value="11"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="[1-9][0-9]{10}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="account" type="ih:Account"/>
                </xs:sequence>
            </xs:complexType>
            <!--  Request Item: INVOICE HEADER - TRANSACTION  -->
            <xs:complexType xmlns:xs="http://www.w3.org/2001/XMLSchema" name="Transaction">
                <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Transaction properties.
                    </xs:documentation>
                </xs:annotation>
                <xs:sequence xmlns:xs="http://www.w3.org/2001/XMLSchema">
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="merchValue">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Merch value, decimal
                                    number in format 15,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,15}.{0,1}\d{0,2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="serviceValue">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Service value, decimal
                                    number in format 15,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
                                <xs:pattern xmlns:xs="http://www.w3.org/2001/XMLSchema" value="\d{1,15}.{0,1}\d{0,2}"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="sums" type="ih:Sums"/>
                    <xs:element xmlns:xs="http://www.w3.org/2001/XMLSchema" name="amountToPay">
                        <xs:simpleType xmlns:xs="http://www.w3.org/2001/XMLSchema">
                            <xs:annotation xmlns:xs="http://www.w3.org/2001/XMLSchema">
                                <xs:documentation xmlns:xs="http://www.w3.org/2001/XMLSchema">Amount to pay, decimal
                                    number in format 15,2
                                </xs:documentation>
                            </xs:annotation>
                            <xs:restriction xmlns:xs="http://www.w3.org/2001/XMLSchema" base="xs:decimal">
                                <xs:whiteSpace xmlns:xs="http://www.w3.org/2001/XMLSchema" value="collapse"/>
                                <xs:fractionDigits xmlns:xs="http://www.w3.org/2001/XMLSchema" value="2"/>
                                <xs:minInclusive xmlns:xs="http://www.w3.org/2001/XMLSchema" value="1"/>
